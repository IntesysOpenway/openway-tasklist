<?xml version='1.0' encoding='UTF-8'?>
<assembly>
	<id>jar</id>
	<formats>
		<format>jar</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/amp/config</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>**/*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>${project.build.directory}/amp/web</directory>
			<outputDirectory>/META-INF</outputDirectory>
			<includes>
				<include>**/*.*</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>${project.build.directory}/classes</directory>
			<outputDirectory>/</outputDirectory>
			<includes>
				<include>**/*.*</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>